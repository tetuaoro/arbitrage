{
	"name": "arbitrage",
	"version": "1.3.0",
	"description": "Make an arbitrage trading bot : sniping & monitoring uniswap fork on polygon network",
	"main": "build/server.js",
	"author": "tetuaoro <tetuaoropro@gmail.com>",
	"license": "MIT",
	"private": false,
	"scripts": {
		"migrate": "truffle migrate --network development",
		"migrate:prod": "truffle migrate --network polygon",
		"start:dev": "NODE_ENV=dev node .",
		"start:prod": "node .",
		"build:server": "tsc --build --clean && tsc && gulp && node minify.js",
		"test:server": "tsc && yarn start:dev",
		"test:prod": "tsc && yarn start:prod",
		"build:client": "tsc -p tsconfig.client.json",
		"start:client": "node dist/client.js",
		"test:client": "yarn build:client && yarn start:client"
	},
	"dependencies": {
		"@sendgrid/mail": "^7.6.0",
		"dayjs": "^1.10.7",
		"dotenv": "^10.0.0",
		"ethers": "^5.5.1",
		"express": "^4.17.2",
		"ink": "^3.2.0",
		"ink-table": "^3.0.0",
		"react": "^17.0.2",
		"socket.io": "^4.4.0",
		"socket.io-client": "^4.4.0"
	},
	"devDependencies": {
		"@node-minify/core": "^6.2.0",
		"@node-minify/jsonminify": "^6.2.0",
		"@truffle/hdwallet-provider": "^2.0.0",
		"@types/express": "^4.17.13",
		"@types/node": "^16.11.12",
		"@types/react": "^17.0.38",
		"@uniswap/v2-periphery": "^1.1.0-beta.0",
		"gulp": "^4.0.2",
		"gulp-terser": "^2.1.0",
		"openzeppelin-solidity": "^4.4.0"
	}
}
